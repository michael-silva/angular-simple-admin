// Place your settings in this file to overwrite default and user settings.
{
    "files.exclude": { 
         "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/.DS_Store": true,
        "node_modules": true,
        "typings": true,
        "**/*.js": { "when": "$(basename).ts"},
        "**/*.js.map": { "when": "$(basename)"}
    }
,
"typescript.check.workspaceVersion": false
}